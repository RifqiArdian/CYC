<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell><Data ss:Type="String">NIM</Data></Cell>
        <Cell><Data ss:Type="String">Nama</Data></Cell>
        <Cell><Data ss:Type="String">Jenis Kelamin</Data></Cell>
        <Cell><Data ss:Type="String">Alamat</Data></Cell>
        <Cell><Data ss:Type="String">Tempat Lahir</Data></Cell>
        <Cell><Data ss:Type="String">Tanggal Lahir</Data></Cell>
        <Cell><Data ss:Type="String">No Telepon</Data></Cell>
        <Cell><Data ss:Type="String">Jurusan</Data></Cell>
        <Cell><Data ss:Type="String">Fakultas</Data></Cell>
        <Cell><Data ss:Type="String">Email</Data></Cell>
        <Cell><Data ss:Type="String">Kepribadian</Data></Cell>
        <Cell><Data ss:Type="String">Keterangan</Data></Cell>
      </Row>
    <% @students.each do |student| %>
      <Row>
        <Cell><Data ss:Type="String"><%= student.nim %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.nama %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.jenis_kelamin %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.alamat %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.tempat_lahir %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.tgl_lahir %></Data></Cell>
        <Cell><Data ss:Type="Number"><%= student.no_telepon %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.jurusan %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.fakultas %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.email %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.kepribadian %></Data></Cell>
        <Cell><Data ss:Type="String"><%= student.keterangan %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>

